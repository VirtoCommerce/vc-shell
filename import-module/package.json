{
  "name": "@vc-shell/import-module",
  "version": "1.0.129",
  "description": "Import module",
  "type": "module",
  "main": "./dist/import-module.js",
  "types": "./dist/src/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/import-module.mjs",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css",
    "./*": "./*"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "type-check": "vue-tsc --noEmit",
    "generate:api-client": "cross-env api-client-generator --color -- APP_PLATFORM_MODULES='[Import]' APP_API_CLIENT_DIRECTORY=./src/api_client/"
  },
  "devDependencies": {
    "@vc-shell/api-client-generator": "^1.0.129",
    "@vc-shell/config-generator": "^1.0.129",
    "@vc-shell/ts-config": "^1.0.129",
    "cross-env": "^7.0.3",
    "typescript": "5.3.3",
    "vite": "4.5.0",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "@vc-shell/framework": "^1.0.129",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
