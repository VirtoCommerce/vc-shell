{
  "name": "vc-app-extend",
  "version": "1.0.179",
  "private": true,
  "scripts": {
    "serve": "cross-env APP_ENV=development vite --force",
    "preview": "vite preview",
    "build": "yarn build:app && yarn build:types",
    "build:app": "cross-env APP_ENV=production vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "type-check": "vue-tsc --noEmit",
    "lint-staged": "lint-staged",
    "lint": "eslint --fix --cache '**/*.{ts,vue}'",
    "generate-api-client": "cross-env api-client-generator  --APP_PLATFORM_MODULES='[VirtoCommerce.MarketplaceVendor]' --APP_API_CLIENT_DIRECTORY=./src/api_client/",
    "release": "tsx scripts/release.ts --dry"
  },
  "workspaces": [
    "./src/api_client"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@laynezh/vite-plugin-lib-assets": "^0.5.13",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vc-shell/api-client-generator": "^1.0.179",
    "@vc-shell/release-config": "^1.0.179",
    "@vc-shell/ts-config": "^1.0.179",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "sass": "^1.69.6",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-mkcert": "^1.17.1",
    "vite-plugin-pwa": "^0.18.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@vc-app-extend/api": "^1.0.113",
    "@vc-app/modules": "^1.0.113",
    "@vc-shell/config-generator": "^1.0.179",
    "@vc-shell/framework": "^1.0.179",
    "@vueuse/core": "^10.7.1",
    "@vueuse/integrations": "^10.7.1",
    "moment": "^2.30.1",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "lint-staged": {
    "*.{js,ts,html,css}": "prettier --write '**/*.{ts,vue}'",
    "*.{js,ts,vue}": "eslint --fix --cache '**/*.{ts,vue}'"
  },
  "packageManager": "yarn@4.0.2"
}
