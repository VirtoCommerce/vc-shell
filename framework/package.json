    "@vueuse/components": "^9.9.0",
    "@vueuse/core": "^9.9.0",
    "@vueuse/integrations": "^9.9.0",
    "client-oauth2": "^4.3.3",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "querystring-es3": "^0.2.1",
    "quill-image-uploader": "^1.2.4",
    "swiper": "^6.8.4",
    "vee-validate": "^4.7.3",
    "vue": "^3.0.0",
    "vue-currency-input": "~2.5.1",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.1.6",
    "vue3-ace-editor": "^2.2.2",
    "vue3-touch-events": "^4.1.0"
  },
  "devDependencies": {
    "@vc-shell/api-client-generator": "^1.0.61",
    "@vc-shell/config-generator": "^1.0.61",
    "sass": "^1.49.0",
    "typescript": "^4.9.5",
    "url-pattern": "^1.0.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
