{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.eol": "\r\n",
  "eslint.validate": [
    "vue",
    "javascript"
  ],
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
  "i18n-ally.localesPaths": [
    "boilerplate/src/locales",
    "framework/ui/locales",
    "apps/vendor-portal/src/locales",
    "framework/core/plugins/i18n",
    "framework/shared/modules/assets-manager/locales",
    "framework/shared/modules/assets/locales",
    "framework/shared/locales",
    "framework/shared/modules/dynamic/locales",
    "framework/shared/pages/InvitePage/locales",
    "framework/shared/pages/LoginPage/locales",
    "framework/shared/pages/ResetPasswordPage/locales",
    "apps/vendor-portal/src/modules/import/locales",
    "apps/vendor-portal/src/modules/marketplace-products/locales",
    "apps/vendor-portal/src/modules/offers/locales",
    "apps/vendor-portal/src/modules/products/locales",
    "apps/vendor-portal/src/modules/orders/locales",
    "apps/vendor-portal/src/modules/rating/locales",
    "apps/vendor-portal/src/modules/settings/locales",
    "apps/vendor-portal/src/modules/dynamic/locales",
    "import-module/src/locales",
    "sample/vc-app/src/locales",
    "sample/vc-app-extend/src/locales",
    "apps/vendor-portal/src/modules/newOffers/locales",
    "apps/vendor-portal/src/modules/newProducts/locales",
    "apps/vendor-portal/src/modules/newTeam/locales",
    "sample/vc-app/src/modules/sample/locales",
    "cli/create-vc-app/src/template/base/src/locales",
    "cli/create-vc-app/src/template/code/blade/src/modules/default/locales"
  ],
  "i18n-ally.keystyle": "nested",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "relative",
  "cSpell.words": [
    "GTIN",
    "Vcmp"
  ]
}
