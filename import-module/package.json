{
  "name": "@vc-shell/import-module",
  "version": "1.0.110",
  "description": "Import module",
  "main": "./dist/import-module.mjs",
  "module": "./dist/import-module.mjs",
  "types": "./dist/src/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/import-module.mjs",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css",
    "./*": "./*"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "type-check": "vue-tsc --noEmit",
    "generate-api-client": "cross-env api-client-generator --stream --no-prefix --color"
  },
  "devDependencies": {
    "@vc-shell/api-client-generator": "^1.0.110",
    "@vc-shell/config-generator": "^1.0.110",
    "cross-env": "^7.0.3",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.19"
  },
  "dependencies": {
    "@vc-shell/framework": "^1.0.110",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
