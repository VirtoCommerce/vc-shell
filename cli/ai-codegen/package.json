{
  "name": "@vc-shell/ai-codegen",
  "version": "1.1.99-alpha.11",
  "description": "AI-powered code generation tools for VC Shell applications with MCP support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vcgen": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./cli": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/schemas",
    "src/examples",
    ".cursorrules-composition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtoCommerce/vc-shell.git",
    "directory": "cli/ai-codegen"
  },
  "homepage": "https://github.com/VirtoCommerce/vc-shell/tree/main/cli/ai-codegen",
  "bugs": {
    "url": "https://github.com/VirtoCommerce/vc-shell/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup && bash scripts/copy-assets.sh",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "mcp:inspect": "npx @modelcontextprotocol/inspector node dist/index.js mcp",
    "mcp:watch": "./watch-mcp-logs.sh",
    "mcp:clean": "pkill -f 'vcshell.*mcp' || true"
  },
  "dependencies": {
    "@babel/core": "^7.25.0",
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.0",
    "@babel/traverse": "^7.25.0",
    "@babel/types": "^7.25.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@vue/compiler-sfc": "^3.5.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dedent": "^1.6.0",
    "diff": "^8.0.2",
    "execa": "^9.6.0",
    "fuzzysort": "^3.1.0",
    "lodash-es": "^4.17.21",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "yaml": "^2.7.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@types/prompts": "^2.4.9",
    "fs-extra": "^11.3.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "vc-shell",
    "codegen",
    "ai",
    "vue",
    "typescript",
    "cli"
  ]
}
