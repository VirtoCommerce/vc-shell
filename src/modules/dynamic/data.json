{
  "settings": {
    "url": "/test",
    "titleTemplate": "Test blade",
    "moduleName": "Test module",
    "icon": "fas fa-file-alt",
    "endpoint": "/api/vcmp/seller/products",
    "toolbar": [
      {
        "id": "refresh",
        "title": "Refresh",
        "icon": "fas fa-save"
      }
    ]
  },
  "content": [
    {
      "type": "form",
      "children": [
        {
          "type": "vc-input",
          "label": "Name",
          "property": "productData.name"
        },
        {
          "type": "vc-select",
          "label": "Test select",
          "property": "categoryId",
          "endpoint": "/api/vcmp/seller/categories/search",
          "optionValue": "id",
          "optionLabel": "name"
        },
        {
          "type": "vc-card",
          "label": "Properties",
          "children": [
            {
              "type": "vc-input",
              "label": "GTIN",
              "property": "gtin"
            },
            {
              "type": "vc-editor",
              "label": "Description",
              "property": "description"
            }
          ]
        },
        {
          "type": "vc-card",
          "label": "Gallery",
          "children": [
            {
              "type": "vc-gallery",
              "property": "productData.images"
            }
          ]
        }
      ]
    },
    {
      "type": "widgets",
      "children": [
        {
          "label": "Offers",
          "icon": "fas fa-tags",
          "navigate": "OffersList",
          "context": "sellerProduct"
        },
        {
          "label": "Assets",
          "icon": "far fa-file",
          "navigate": "AssetsManager"
        }
      ]
    }
  ]
}
