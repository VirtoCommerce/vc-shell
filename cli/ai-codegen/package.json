{
  "name": "@vc-shell/ai-codegen",
  "version": "0.1.0",
  "description": "AI-powered code generation tools for VC Shell applications with MCP support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vcgen": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/schemas",
    "src/examples",
    ".cursorrules-composition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VirtoCommerce/vc-shell.git",
    "directory": "cli/ai-codegen"
  },
  "homepage": "https://github.com/VirtoCommerce/vc-shell/tree/main/cli/ai-codegen",
  "bugs": {
    "url": "https://github.com/VirtoCommerce/vc-shell/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean && bash scripts/copy-assets.sh",
    "dev": "tsup src/index.ts --format esm --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "mcp:inspect": "npx @modelcontextprotocol/inspector node dist/index.js mcp"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dedent": "^1.6.0",
    "diff": "^8.0.2",
    "execa": "^9.6.0",
    "fuzzysort": "^3.1.0",
    "lodash-es": "^4.17.21",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "vc-shell",
    "codegen",
    "ai",
    "vue",
    "typescript",
    "cli"
  ],
  "author": "VC Shell Team",
  "license": "MIT"
}
