{
  "name": "vc-app",
  "version": "1.0.119",
  "private": true,
  "scripts": {
    "bootstrap": "yarn build:api && yarn build:modules-library",
    "serve": "cross-env APP_ENV=development vite --force",
    "preview": "vite preview",
    "build": "yarn bootstrap && yarn build:app & yarn build:types",
    "build:app": "cross-env APP_ENV=production vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:api": "tsc -p ./src/api_client",
    "publish": "yarn workspaces foreach -v --from '{@vc-app/modules,@vc-app/api}' npm publish",
    "build:modules-library": "yarn build:modules && yarn build:types-modules",
    "build:modules": "vite build --config ./src/modules/vite.config.ts",
    "build:types-modules": "vue-tsc --declaration --emitDeclarationOnly --p ./src/modules/tsconfig.json --outDir dist/modules/types",
    "type-check": "vue-tsc --noEmit",
    "lint-staged": "lint-staged",
    "generate:api-client": "cross-env api-client-generator --color -- APP_PLATFORM_MODULES='[MarketplaceVendor]' APP_API_CLIENT_DIRECTORY=./src/api_client/",
    "release": "tsx scripts/release.ts --dry"
  },
  "workspaces": [
    "src/api_client",
    "src/modules"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vc-shell/api-client-generator": "^1.0.119",
    "@vc-shell/release-config": "^1.0.119",
    "@vc-shell/ts-config": "^1.0.119",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "buffer-esm": "^1.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "querystring-es3": "^0.2.1",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-plugin-pwa": "^0.16.5",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.8.20"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@vc-app/api": "^1.0.113",
    "@vc-app/modules": "^1.0.113",
    "@vc-shell/config-generator": "^1.0.119",
    "@vc-shell/framework": "^1.0.119",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.1.2",
    "moment": "^2.29.4",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "packageManager": "yarn@3.6.4"
}
