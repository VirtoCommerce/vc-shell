{
  "name": "@vc-shell/framework",
  "version": "1.1.9",
  "type": "module",
  "main": "./dist/framework.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist",
    "dist/locales/*.json",
    "tailwind.config.ts",
    "ui",
    "core",
    "shared"
  ],
  "exports": {
    ".": {
      "import": "./dist/framework.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css",
    "./dist/locales/*": "./dist/locales/*",
    "./tailwind.config": "./tailwind.config.ts",
    "./*": "./*"
  },
  "scripts": {
    "build": "shx rm -rf ./dist && tsx scripts/build.mts",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "type-check": "vue-tsc --noEmit",
    "generate-icons": "tsx scripts/generate-icons.mts",
    "check-locales": "tsx scripts/check-locales.mts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",
    "@fontsource/plus-jakarta-sans": "^5.1.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@headlessui/vue": "^1.7.19",
    "@microsoft/signalr": "^8.0.0",
    "@vavt/cm-extension": "^1.5.0",
    "@vee-validate/i18n": "^4.12.4",
    "@vee-validate/rules": "^4.12.4",
    "@vuepic/vue-datepicker": "^9.0.2",
    "@vueuse/components": "^10.7.1",
    "@vueuse/core": "^10.7.1",
    "@vueuse/integrations": "^10.7.1",
    "bootstrap-icons": "^1.11.3",
    "core-js": "^3.35.0",
    "dompurify": "^3.0.11",
    "iso-639-1": "^3.1.0",
    "lucide-vue-next": "^0.503.0",
    "material-symbols": "^0.31.2",
    "md-editor-v3": "^4.20.2",
    "moment": "^2.30.1",
    "normalize.css": "^8.0.1",
    "swiper": "^6.8.4",
    "truncate-html": "^1.1.1",
    "vee-validate": "^4.12.4",
    "vue": "^3.5.13",
    "vue-currency-input": "^3.0.5",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.2.5",
    "vue3-application-insights": "^1.1.0",
    "vue3-touch-events": "^4.1.8",
    "whatwg-fetch": "^3.6.19"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@types/dompurify": "^3.0.5",
    "@vc-shell/api-client-generator": "^1.1.9",
    "@vc-shell/config-generator": "^1.1.9",
    "@vc-shell/ts-config": "^1.1.9",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.5",
    "cypress-signalr-mock": "^1.5.0",
    "esbuild": "^0.25.3",
    "sass": "^1.87.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vitest": "^1.3.1",
    "vue-component-type-helpers": "^2.2.10",
    "vue-tsc": "^2.2.10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "npmTag": "next"
}
