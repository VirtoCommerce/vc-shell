{
  "name": "@vc-shell/root",
  "version": "1.0.313",
  "private": true,
  "repository": "https://github.com/VirtoCommerce/vc-shell",
  "scripts": {
    "postinstall": "husky install",
    "publish": "yarn workspaces foreach -v --worktree --from '{@vc-shell/config-generator,@vc-shell/api-client-generator,@vc-shell/framework,@vc-shell/create-vc-app,@vc-shell/release-config,@vc-shell/ts-config}' npm publish",
    "lint": "eslint --fix --cache 'framework/**/*.{ts,vue}' 'cli/**/*.{ts,vue}' 'configs/**/*.{ts,vue}'",
    "lint-staged": "lint-staged",
    "build": "yarn workspaces foreach -v --worktree --from '{@vc-shell/config-generator,@vc-shell/api-client-generator,@vc-shell/create-vc-app,@vc-shell/framework,@vc-shell/release-config}' run build",
    "build-framework": "yarn workspace @vc-shell/framework run build",
    "build-cli:config": "yarn workspace @vc-shell/config-generator run build",
    "build-cli:api-client": "yarn workspace @vc-shell/api-client-generator run build",
    "build-cli:create-vc-app": "yarn workspace @vc-shell/create-vc-app run build",
    "build-config:release-config": "yarn workspace @vc-shell/release-config run build",
    "generate:api-client": "yarn workspace @vc-shell/framework exec cross-env api-client-generator --APP_PLATFORM_MODULES='[VirtoCommerce.Platform]' --APP_API_CLIENT_DIRECTORY=./core/api/",
    "storybook-build": "cross-env APP_ENV=production storybook build .storybook/assets",
    "storybook-serve": "storybook dev .storybook/assets -p 6006 --no-open",
    "release": "tsx scripts/release.ts",
    "storybook-compress": "vite-node --mode production scripts/compress-storybook.ts --"
  },
  "workspaces": [
    "apps/*",
    "framework",
    "cli/*",
    "configs/*",
    "sample/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-actions": "^8.0.4",
    "@storybook/addon-backgrounds": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/components": "^8.0.4",
    "@storybook/core-events": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@storybook/theming": "^8.0.4",
    "@storybook/vue3": "^8.0.4",
    "@storybook/vue3-vite": "^8.0.4",
    "@types/adm-zip": "^0.5.5",
    "@types/lodash": "^4.17.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/prismjs": "^1.26.3",
    "@types/serialize-javascript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "chalk": "^2.4.2",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "~1.69.6",
    "serialize-javascript": "^6.0.2",
    "shx": "^0.3.4",
    "storybook": "^8.0.4",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "~5.3.3",
    "vite": "5.3.6",
    "vite-node": "^1.3.1",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-mkcert": "^1.17.1",
    "vite-plugin-pwa": "^0.18.1",
    "vue": "^3.4.19",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3"
  },
  "lint-staged": {
    "*.{js,ts,html,css}": "prettier --write 'framework/**/*.{ts,vue}' 'cli/**/*.{ts,vue}' 'configs/**/*.{ts,vue}'",
    "*.{js,ts,vue}": "eslint --fix --cache 'framework/**/*.{ts,vue}' 'cli/**/*.{ts,vue}' 'configs/**/*.{ts,vue}'"
  },
  "packageManager": "yarn@4.0.2"
}
