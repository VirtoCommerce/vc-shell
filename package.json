{
  "name": "app-vendor-portal",
  "version": "1.0.66",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "serve": "cross-env APP_ENV=development vite",
    "preview": "vite preview",
    "build": "yarn build:app & yarn build:types",
    "build:app": "cross-env APP_ENV=production vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "type-check": "vue-tsc --noEmit",
    "lint-staged": "lint-staged",
    "generate-api-client": "cross-env APP_API_PLATFORM_URL=https://vcmp-dev.paas.govirto.com/ api-client-generator --color",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "cross-env-shell \"yarn version --new-version $npm_package_version --no-commit-hooks --ignore-scripts --no-git-tag-version --yes && git add .\"",
    "postversion": "git push origin --tags"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vc-shell/api-client-generator": "^1.0.60",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "~8.22.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^11.0.1",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.9.5",
    "url-pattern": "^1.0.3",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.4.2",
    "vite-plugin-mkcert": "^1.10.1",
    "vite-plugin-pwa": "^0.14.4",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "~1.0.24",
    "yarn": "^1.22.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@microsoft/signalr": "^5.0.7",
    "@vc-shell/config-generator": "^1.0.60",
    "@vc-shell/framework": "^1.0.60",
    "moment": "^2.29.1",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "^4.7.3",
    "vue": "^3.0.0",
    "vue-router": "^4.1.6"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  }
}
