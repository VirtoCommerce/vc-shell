{
  "name": "@vc-shell/root",
  "version": "1.0.126",
  "private": true,
  "repository": "https://github.com/VirtoCommerce/vc-shell",
  "scripts": {
    "postinstall": "husky install",
    "publish": "yarn workspaces foreach -v --from '{@vc-shell/config-generator,@vc-shell/api-client-generator,@vc-shell/framework,@vc-shell/create-vc-app,@vc-shell/import-module,@vc-shell/release-config,@vc-shell/ts-config}' npm publish",
    "lint": "eslint --fix --cache 'framework/**/*.{ts,vue}' 'import-module/**/*.{ts,vue}'",
    "lint-staged": "lint-staged",
    "build": "yarn workspaces foreach -v --from '{@vc-shell/config-generator,@vc-shell/api-client-generator,@vc-shell/create-vc-app,@vc-shell/framework,@vc-shell/import-module,@vc-shell/release-config}' run build",
    "build-framework": "yarn workspace @vc-shell/framework run build",
    "build:import-module": "yarn workspace @vc-shell/import-module run build",
    "build-cli:config": "yarn workspace @vc-shell/config-generator run build",
    "build-cli:api-client": "yarn workspace @vc-shell/api-client-generator run build",
    "build-cli:create-vc-app": "yarn workspace @vc-shell/create-vc-app run build",
    "build-config:release-config": "yarn workspace @vc-shell/release-config run build",
    "generate:api-client": "yarn workspace @vc-shell/framework exec cross-env api-client-generator --APP_PLATFORM_MODULES='[Platform]' --APP_API_CLIENT_DIRECTORY=./core/api/",
    "storybook-build": "cross-env APP_ENV=production storybook build -s .storybook/assets",
    "storybook-serve": "storybook dev .storybook/assets -p 6006",
    "release": "tsx scripts/release.ts"
  },
  "workspaces": [
    "apps/*",
    "framework",
    "cli/*",
    "configs/*",
    "import-module",
    "sample/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-actions": "^7.0.17",
    "@storybook/addon-backgrounds": "^7.0.17",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/components": "^7.0.17",
    "@storybook/core-client": "^7.0.17",
    "@storybook/core-events": "^7.0.17",
    "@storybook/theming": "^7.0.17",
    "@storybook/vue3": "^7.0.17",
    "@storybook/vue3-vite": "^7.0.17",
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "chalk": "^2.4.2",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-define-config": "^1.24.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "path": "^0.12.7",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "shx": "^0.3.4",
    "storybook": "^7.0.17",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-plugin-pwa": "^0.14.7",
    "vue": "^3.3.7",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.8.20"
  },
  "lint-staged": {
    "*.{js,ts,html,css}": "prettier --write",
    "*.{js,ts,vue}": "eslint --fix --cache"
  },
  "packageManager": "yarn@3.6.4"
}
