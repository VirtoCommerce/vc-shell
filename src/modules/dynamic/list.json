{
  "settings": {
    "url": "/test-list",
    "titleTemplate": "Test list blade",
    "moduleName": "Test module",
    "icon": "fas fa-file-alt",
    "endpoint": "/api/vcmp/seller/products",
    "toolbar": [
      {
        "id": "save",
        "title": "Save",
        "icon": "fas fa-save"
      }
    ]
  },
  "content": [
    {
      "type": "grid",
      "columns": [
        {
          "id": "imgSrc",
          "title": "Image",
          "type": "image"
        },
        {
          "id": "name",
          "title": "Name",
          "alwaysVisible": true
        },
        {
          "id": "createdDate",
          "title": "Created date",
          "type": "date-ago"
        },
        {
          "id": "isPublished",
          "title": "Is published",
          "type": "status-icon"
        },
        {
          "id": "status",
          "title": "Status",
          "alwaysVisible": true
        },
        {
          "id": "gtin",
          "field": "productData.gtin",
          "title": "GTIN",
          "alwaysVisible": true
        },
        {
          "id": "productType",
          "field": "productData.productType",
          "title": "Product type"
        }
      ]
    }
  ]
}
