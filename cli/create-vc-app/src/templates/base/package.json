{
  "name": "app-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "serve": "cross-env APP_ENV=development vite --force",
    "preview": "vite preview",
    "build": "yarn build:app && yarn build:types",
    "build:app": "cross-env APP_ENV=production vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "type-check": "vue-tsc --noEmit",
    "lint-staged": "lint-staged",
    "generate:api-client": "cross-env api-client-generator --color -- APP_PLATFORM_MODULES='[]' APP_API_CLIENT_DIRECTORY=./src/api_client/",
    "release": "tsx scripts/release.ts --dry"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@laynezh/vite-plugin-lib-assets": "^0.5.6",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vc-shell/api-client-generator": "^1.0.128",
    "@vc-shell/release-config": "^1.0.128",
    "@vc-shell/ts-config": "^1.0.128",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.14",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "querystring-es3": "^0.2.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.13.0",
    "typescript": "5.3.3",
    "vite": "4.5.0",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-plugin-pwa": "^0.17.3",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@vc-shell/config-generator": "^1.0.128",
    "@vc-shell/framework": "^1.0.128",
    "@vueuse/core": "^10.7.0",
    "@vueuse/integrations": "^10.7.0",
    "moment": "^2.29.4",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "^4.11.8",
    "vue": "3.3.13",
    "vue-router": "^4.2.5"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "packageManager": "yarn@3.6.4"
}
