{
  "settings": {
    "url": "/products-j",
    "titleTemplate": "Products",
    "moduleName": "Products",
    "composable": "useProducts",
    "icon": "fas fa-file-alt",
    "toolbar": [
      {
        "id": "add",
        "title": "Add (Optional)"
      }
    ]
  },
  "content": [
    {
      "type": "grid",
      "columns": [
        {
          "id": "imgSrc",
          "title": "Image",
          "type": "image"
        },
        {
          "id": "name",
          "title": "Name",
          "sortable": true,
          "alwaysVisible": true
        },
        {
          "id": "createdDate",
          "title": "Created date",
          "sortable": true,
          "type": "date-ago"
        },
        {
          "id": "isPublished",
          "title": "Is published",
          "sortable": true,
          "type": "status-icon"
        },
        {
          "id": "status",
          "title": "Status",
          "sortable": true,
          "alwaysVisible": true
        },
        {
          "id": "gtin",
          "field": "productData.gtin",
          "title": "GTIN",
          "alwaysVisible": true
        },
        {
          "id": "productType",
          "field": "productData.productType",
          "title": "Product type"
        }
      ]
    }
  ]
}
